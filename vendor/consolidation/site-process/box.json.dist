{
  "alias": "remo.phar",
  "chmod": "0755",
  "compactors": [],
  "directories": ["src"],
  "files": ["remo", "README.md", "VERSION"],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "test",
        "tests",
        "Test",
        "Tests",
        "Tester"
      ],
      "in": "vendor"
    }
  ],
  "git-commit": "git-commit",
  "git-version": "git-version",
  "output": "remo.phar",
   "main": "remo",
  "stub": true
}
