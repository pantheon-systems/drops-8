parameters:
    level: 3
    paths:
        - %rootDir%/../../../lib
        - %rootDir%/../../../tests

    ignoreErrors:
        - '#Doctrine\\Common\\Reflection\\StaticReflection[a-zA-Z0-9_]+::__construct\(\) does not call parent constructor from Reflection[a-zA-Z0-9_]+#'

includes:
    - vendor/phpstan/phpstan-phpunit/extension.neon
